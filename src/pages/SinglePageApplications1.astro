---
import Card from "../components/Card.astro";
import CardContainer from "../components/CardContainer.astro";
import Comentario from "../components/Comentario.astro";
import TituloLocal from "../components/TituloLocal.astro";
import MainLayout from "../layouts/MainLayout.astro";
---

<MainLayout name="SinglePageApplications1">
  <TituloLocal contenido="Single page applications" />
  <CardContainer>
    <Card
      nombre="Usuario"
      contenido="Interactua con la app (presiona botones)"
    />
    <Card nombre="App" contenido="Modifica su contenido" />
  </CardContainer>
  <Comentario contenido="Una sola ruta: https://www.aplicacion.com/" />
</MainLayout>
