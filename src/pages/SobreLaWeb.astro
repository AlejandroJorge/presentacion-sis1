---
import MainLayout from "../layouts/MainLayout.astro";
import Card from "../components/Card.astro";
import CardContainer from "../components/CardContainer.astro";
import TituloLocal from "../components/TituloLocal.astro";
import ContentContainer from "../components/ContentContainer.astro";
---

<MainLayout name="SobreLaWeb">
  <TituloLocal contenido="Como funciona la web in a nutshell" />
  <ContentContainer>
    <CardContainer>
      <Card nombre="Servidor" contenido="Envia informacion" />
      <Card nombre="Navegador" contenido="Procesa la informacion" />
      <Card nombre="Display" contenido="Muestra la informacion" />
    </CardContainer>
  </ContentContainer>
</MainLayout>
